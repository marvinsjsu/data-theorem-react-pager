{
  "name": "data-theorem-pager",
  "version": "1.0.0",
  "description": "a library with Pager component",
  "main": "/dist/index.js",
  "scripts": {
    "build": "webpack",
    "start": "webpack-dev-server --mode development",
    "transpile": "./node_modules/.bin/babel src -d dist --copy-files",
    "prepublishOnly": "npm run transpile && npm run compile:sass",
    "local:test": "npm run transpile && npm run compile:sass",
    "compile:sass": "node-sass src/sass/main.scss dist/css/main.css",
    "test": "jest --verbose",
    "test:watch": "jest --watch --verbose",
    "test:coverage": "jest --verbose --coverage"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "keywords": [],
  "author": "marvinsjsu@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.5.5",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "@babel/plugin-proposal-class-properties": "^7.5.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "jest": "^24.8.0",
    "react-test-renderer": "^16.8.6"
  }
}
